package foo.bar;

import "other.aur"

// Some comment

bus C1: biz.R;
bus C2: int;
bus Out: int;

intf Clock {
   tick(): int;
}

comp Adder implements Clock {
    in {
       a: int;
       int b;
    }

    out {
       int c;
    }

    methods {
      void m1 [from {a,b}, to {c}]
      void tick [every 20ms, to {c}]
    }

    storage {
       int factor;
    }

    connect {
       C1 -> a;
       C2 -> b;
       c -> Out
    }
}
